Compression yes

Include config.d/*

Host *
  ServerAliveInterval 60
  ServerAliveCountMax 3

Host uberspace
   User l4usen
   Hostname devico.uberspace.de

Host freiburg
   User leonard
   Hostname freiburg.lausen.nl

Host hoboken
   User leonard
   Hostname hoboken.lausen.nl

Host hoboken-initramfs
	Hostname hoboken.lausen.nl
	User root
	UserKnownHostsFile ~/.ssh/known_hosts.initramfs

Host *.*.i-*
	ProxyCommand sh -c "profile=$(echo %h | cut -d. -f1); region=$(echo %h | cut -d. -f2); instance=$(echo %h | cut -d. -f3); aws --profile \$profile --region \$region ssm start-session --target \$instance --document-name AWS-StartSSHSession --parameters portNumber=%p"
